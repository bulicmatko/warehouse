  "explorer.openEditors.visible": 0,
  "search.smartCase": true,
  "editor.fontSize": 11,
  "editor.lineHeight": 22,
  "editor.letterSpacing": 0.5,
  "editor.fontFamily": "Monaco",
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 120, 160, 240],
  "editor.formatOnSave": false,
  "editor.snippetSuggestions": "top",
  "editor.renderLineHighlight": "all",
  "editor.minimap.enabled": false,
  "editor.tokenColorCustomizations": {
    "[One Dark Theme]": {
      // "comments": "#229977"
    }
  },
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.lineHeight": 1,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.shell.osx": "zsh",
  "terminal.integrated.drawBoldTextInBrightColors": true,
  "files.exclude": {
    "**/.idea": true,
    "**/.vscode": true,
    "**/node_modules": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.promptToSaveFilesBeforeCommit": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "cSpell.userWords": [
    "bulic",
    "bulicmatko",
    "devtool",
    "devtools",
    "firepack",
    "firestore",
    "gmail",
    "matko",
    "mobx",
    "node",
    "reselect",
    "topbar",
    "unmount"
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "svgviewer.enableautopreview": false,
  "gitProjectManager.baseProjectsFolders": [
    "~/Development/Personal",
    "~/Development/Work"
  ],
  "gitProjectManager.openInNewWindow": false,
  "gitProjectManager.maxDepthRecursion": 10,
  "gitProjectManager.ignoredFolders": ["**/node_modules"],
  "gitlens.keymap": "chorded",
  "gitlens.codeLens.scopes": ["blocks", "containers", "document"],
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.currentLine.over": "line",

  "todohighlight.isCaseSensitive": false,
  "todohighlight.toggleURI": true,
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": false
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    }
  },
  "npm.enableScriptExplorer": true,
  "window.zoomLevel": 0,
  "gitlens.historyExplorer.enabled": true
}
